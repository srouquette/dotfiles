{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{71160544-14d8-4194-af25-d05feeac7233}",
    "multiLinePasteWarning": false,
    "newTabMenu":
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles":
    {
        "defaults":
        {
            "adjustIndistinguishableColors": "never",
            "antialiasingMode": "cleartype",
            "bellStyle": "window",
            "closeOnExit": "graceful",
            "colorScheme": "rose-pine",
            "font":
            {
                "face": "FiraCode Nerd Font Propo"
            },
            // "cursorShape": "vintage",
            "backgroundImage": "%USERPROFILE%/.config/windows-terminal/rose-pine-bg.jpg",
            // "backgroundImageStretchMode": "fill",
            // "backgroundImageAlignment": "topRight",
            "backgroundImageOpacity": 0.2,
            "opacity": 60,
            "useAcrylic": true,
            "useAtlasEngine": true
        },
        "list":
        [
            {
                "commandline": "C:/msys64/msys2_shell.cmd -defterm -here -no-start -ucrt64 -shell zsh",
                "environment":
                {
                    "MSYS": "winsymlinks:nativestrict"
                },
                "guid": "{71160544-14d8-4194-af25-d05feeac7233}",
                "icon": "C:/msys64/ucrt64.ico",
                "name": "UCRT64 / MSYS2",
                "startingDirectory": "C:/msys64/home/%USERNAME%"
            },
            {
                "commandline": "%USERPROFILE%/scoop/apps/archwsl/current/Arch.exe",
                "icon": "%USERPROFILE%/scoop/apps/archwsl/current/Arch.exe",
                "startingDirectory": "//wsl$/Arch/home",
                "guid": "{71160544-14d8-4194-af25-d05feeac7230}",
                "name": "ArchWSL"
            },
            {
                "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
                "name": "Windows PowerShell",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "commandline": "%SystemRoot%\\System32\\cmd.exe",
                "hidden": true,
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "name": "Invite de commandes",
                "startingDirectory": "%USERPROFILE%"
            }
        ]
    },
